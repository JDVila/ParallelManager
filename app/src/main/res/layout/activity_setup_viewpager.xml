<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
              android:orientation="vertical"
    >

    <github.chenupt.springindicator.SpringIndicator
        xmlns:app="http://schemas.android.com/apk/res-auto"
        app:siTextSize="18sp"
        app:siTextColor="@color/purpleDark"
        app:siSelectedTextColor="@android:color/white"
        app:siIndicatorColor="@color/purpleMedium"
        app:siRadiusMax="16dp"
        android:id="@+id/activity_setup_springindicator"
        android:layout_width="match_parent"
        android:layout_height="48dp"/>

    <nyc.c4q.rusili.parallelmanager.utility.viewpager.FixedViewPager
        android:id="@+id/activity_setup_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

</LinearLayout>